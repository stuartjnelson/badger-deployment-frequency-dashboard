{
  "name": "semantic-commit-dashboard",
  "version": "0.1.0",
  "description": "A dashboard for release data based on semantic commit messages",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "netlify dev",
    "deploy": "netlify deploy --prod",
    "serve": "netlify dev --port 3000",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write .",
    "test": "jest"
  },
  "dependencies": {
    "@netlify/blobs": "^8.1.0",
    "@netlify/functions": "^1.0.0",
    "@octokit/rest": "^20.0.0",
    "crypto": "^1.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-react": "^7.37.2",
    "globals": "^15.13.0",
    "jest": "^29.0.0",
    "prettier": "^3.0.0",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.18.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "git",
    "releases",
    "Netlify",
    "GitHub",
    "TypeScript",
    "webhooks"
  ],
  "license": "MIT"
}
